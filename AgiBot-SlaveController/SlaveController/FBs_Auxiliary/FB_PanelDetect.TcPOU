      <LineId Id="9" Count="0" />
    </LineIds>
    <LineIds Name="FB_PanelDetect.run">
      <LineId Id="39" Count="5" />
      <LineId Id="15" Count="0" />
      <LineId Id="45" Count="0" />
    </LineIds>
  </POU>
</TcPlcObject>