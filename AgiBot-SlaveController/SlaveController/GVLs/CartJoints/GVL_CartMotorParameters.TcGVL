    <Declaration><![CDATA[{attribute 'global_init_slot' := '49991'}
{attribute 'qualified_only'}
VAR_GLOBAL CONSTANT
	// encoder resolution
	//TODO: resolution of joint 2/3 need verification
	g_incEncoderRes :ARRAY[1..g_cartJntNum] OF DINT :=[2(65536), 4*2500, 4*2500, 4*2500, 3(65536)];
	g_absEncoderRes :ARRAY[1..g_cartJntNum] OF DINT :=[1, 1, 1, 1, 8192, 16777215, 8192,16777215];
	
	// reduction ratio
	g_motorReduction :ARRAY[1..g_cartJntNum] OF LREAL :=
		[100.75*2*pi/0.002, 100.75*2*pi/0.002, 160, 160, 
		 5*(2*pi)/0.005, 676.0/49*356, 299.0/14*(2*pi)/0.020, 676.0/49*238];
