	g_minJntPos: vec8d:=[0, 0, 1, 1, 0, -30*g_deg2Rad, 0.002, -160*g_deg2Rad];	
	g_maxJntPos: vec8d:=[0.053, 0.053, 1, 1,  0.68, 30*g_deg2Rad, 0.498, 160*g_deg2Rad];	
	
	// joint velocity limit
	g_maxJntVel :ARRAY[1..g_cartJntNum] OF LREAL :=[0.0025, 0.0025, 2.3, 2.3, 0.016, 5*g_deg2Rad, 0.050, 5*g_deg2Rad];
	g_maxJntAcc :ARRAY[1..g_cartJntNum] OF LREAL :=[0.0025 * 2, 0.0025 * 2, 2.3 * 2, 2.3 * 2, 0.016 * 2, 5*g_deg2Rad * 2, 0.050 * 2, 5*g_deg2Rad * 2];
