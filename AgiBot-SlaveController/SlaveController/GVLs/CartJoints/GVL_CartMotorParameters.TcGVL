	g_motorMaxCurrent :ARRAY[1..g_cartJntNum] OF LREAL :=[ 41200,41200,50000,50000,  33000,41200,41200,41200];//mA
	
	// motor and absEncoder direction
	g_motorDir :ARRAY[1..g_cartJntNum] OF INT :=[1, 1, 1, 1, 1, 1, -1, -1];
																												 
