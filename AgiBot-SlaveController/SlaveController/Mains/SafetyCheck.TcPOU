<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4024.11">
  <POU Name="SafetyCheck" Id="{54e6e883-3f22-4a40-9066-2b433bf8009c}" SpecialFunc="None">
    <Declaration><![CDATA[PROGRAM SafetyCheck
VAR
	// slave robot information, including arm status and cartesian data
	m_slaveStatus AT %I* :ST_slaveStatus;
	
	// slave robot joint data
	m_slaveJointData AT %I* :ST_slaveJointData;
	
	// setup robot information
	m_setupStatus AT %I* :ST_SetupStatus;
	
	// setup robot joint data
	m_setupJointsData AT %I* :ST_SetupJointData;
	
	// error check result
	m_safetyCheckData AT %Q* :ST_SafetyData;
	
	//emergency stop and stop botton
	m_stopButton AT %I*: BOOL;
	m_emergencyStopButton AT %I*: BOOL;
	
	//warning tone
	m_warningTone AT %Q*:ARRAY [1..4] OF BOOL;
	
	m_initFlag:bool;
	m_cartCheck :FB_CartCheck;
	m_setupArmCheck:FB_SetupArmCheck;
	m_slaveArmCheck:FB_SlaveArmCheck;
	
	m_SafetyCheck : Fb_SafetyCheck;
	i: INT;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[// reset data
IF FALSE = m_initFlag  THEN
	FOR i:=1  TO g_sArmNum DO 
		m_safetyCheckData.m_slaveErrFlag[i]:=FALSE;
		m_safetyCheckData.m_setupErrFlag[i]:=FALSE;
		m_safetyCheckData.m_cartErrFlag:=FALSE;
	END_FOR
	m_initFlag:=TRUE;
END_IF

m_slaveArmCheck.check(m_slaveStatus,m_slaveJointData, o_errorFlag=>m_safetyCheckData.m_slaveErrFlag);
m_setupArmCheck.check(m_setupStatus,m_setupJointsData, o_errorFlag=>m_safetyCheckData.m_setupErrFlag);]]></ST>
    </Implementation>
    <Method Name="armSafetyCheckAllowed" Id="{c2ebef6f-e712-49e3-9955-377a0083e93a}">
      <Declaration><![CDATA[METHOD armSafetyCheckAllowed : BOOL
VAR_INPUT
END_VAR
VAR
	i , j : UDINT; 
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[
FOR i := 1 TO g_maxArmNum DO
	FOR j := 1 TO g_maxArmJoint DO
		IF j >= 5 THEN
			//fault code 16#3003 
			m_SafetyCheck.checkPermit[i].linkEncoder[j] := m_setupJointsData.m_driveInput[i][j].m_statusWord<>0;
			//fault code 16#3002
			m_SafetyCheck.checkPermit[i].currentIsNotUpdate[j] := m_setupJointsData.m_driveInput[i][j].m_statusWord<>0 AND j > 2;
			//fault code 16#2002
			m_SafetyCheck.checkPermit[i].followErr[j] := m_setupJointsData.m_driveInput[i][j].m_statusWord = DriverStatusWord_OP AND j > 2;
		ELSE
			//fault code 16#3003 
			m_SafetyCheck.checkPermit[i].linkEncoder[j] := m_slaveJointData.m_driveInput[i][j-g_setupJntNum].m_statusWord<>0;
			//fault code 16#3002 
			m_SafetyCheck.checkPermit[i].currentIsNotUpdate[j] := m_slaveJointData.m_driveInput[i][j].m_statusWord<>0 AND j > 2;
			//fault code 16#2002
			m_SafetyCheck.checkPermit[i].followErr[j] := m_slaveJointData.m_driveInput[i][j].m_statusWord = DriverStatusWord_OP AND j > 2;
		END_IF
		//fault code 16#3002
		m_SafetyCheck.checkPermit[i].JntStatus[j] := TRUE;
		//fault code 16#3004
		m_SafetyCheck.checkPermit[i].DriveErr[j] := TRUE;
		
		m_SafetyCheck.checkPermit[i].JntVel[j] := TRUE;
		
		m_SafetyCheck.checkPermit[i].DualEncoderDev[j] := TRUE;
		
		m_SafetyCheck.checkPermit[i].JntCurrent[j] := TRUE;
		m_SafetyCheck.checkPermit[i].FixedPointErr := m_slaveStatus.m_armState[i] = SlaveArmState_MasterSlave;
		
	END_FOR
END_FOR
]]></ST>
      </Implementation>
    </Method>
    <Method Name="armSafetyCheckInputCast" Id="{598e934b-b439-4bef-985d-1ea697c2fcb0}">
      <Declaration><![CDATA[METHOD armSafetyCheckInputCast : BOOL
VAR_INPUT
END_VAR

VAR
	i : UDINT; 
	j : INT;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[FOR i := 1 TO g_maxArmNum DO
	//emergency stop 、stop button and ui stop 
	m_SafetyCheck.checkVariable[i].EmergencyStop := m_emergencyStopButton;
	m_SafetyCheck.checkVariable[i].PCStopButton := GVL_com_upper_PC.stop_button;
	m_SafetyCheck.checkVariable[i].SlaveStopButton := m_stopButton;
	//m_SafetyCheck.AlarmVar[i].MasterStopButton
	
	//setup arm safety check data input mapping
	FOR j := 1 TO g_setupJntNum DO
		m_SafetyCheck.checkVariable[i].m_curLinkPos[j] := m_setupJointsData.m_curLinkPos[i][j];
		m_SafetyCheck.checkVariable[i].m_curJntPos[j] := m_setupJointsData.m_curJntPos[i][j];
		m_SafetyCheck.checkVariable[i].m_driverErrorWordIO[j] := m_setupJointsData.m_driveInput[i][j].m_driverError;
		m_SafetyCheck.checkVariable[i].m_linkEncoderIO[j] := m_setupJointsData.m_driveInput[i][j].m_linkEncoder;
		m_SafetyCheck.checkVariable[i].m_jntStatusWordIO[j] := m_setupJointsData.m_driveInput[i][j].m_statusWord;
		m_SafetyCheck.checkVariable[i].m_curJntTorqueIO[j] := m_setupJointsData.m_driveInput[i][j].m_current;
		m_SafetyCheck.checkVariable[i].m_motPhieErr[j] := m_SafetyCheck.electriAngleDev(m_setupJointsData.m_driveInput[i][j].m_electriAngleDev);
		m_SafetyCheck.checkVariable[i].m_cmdJntPos[j] := m_setupJointsData.m_cmdJntPos[i][j];
		m_SafetyCheck.checkVariable[i].m_curJntVel[j] := m_setupJointsData.m_curJntVel[i][j];
		m_SafetyCheck.checkVariable[i].m_filtJntCurrent[j] := m_setupJointsData.m_filtJntCurrent[i][j];
		m_SafetyCheck.checkVariable[i].m_EtherCATState[j] := m_SafetyCheck.EtherCATVerify(g_ArmNetID[i],g_JointNetPort[j]);

	END_FOR
	
	//slave arm safety check data input mapping
	FOR j := 1 TO g_sJntNum DO
		m_SafetyCheck.checkVariable[i].m_curLinkPos[j+g_setupJntNum] := m_slaveJointData.m_curLinkPos[i][j];
		m_SafetyCheck.checkVariable[i].m_curJntPos[j+g_setupJntNum] := m_slaveJointData.m_curJntPos[i][j];
		m_SafetyCheck.checkVariable[i].m_driverErrorWordIO[j+g_setupJntNum] := m_slaveJointData.m_driveInput[i][j].m_driverError;
		m_SafetyCheck.checkVariable[i].m_linkEncoderIO[j+g_setupJntNum] := m_slaveJointData.m_driveInput[i][j].m_linkEncoder;
		m_SafetyCheck.checkVariable[i].m_jntStatusWordIO[j+g_setupJntNum] := m_slaveJointData.m_driveInput[i][j].m_statusWord;
		m_SafetyCheck.checkVariable[i].m_curJntTorqueIO[j+g_setupJntNum] := m_slaveJointData.m_driveInput[i][j].m_current;
		m_SafetyCheck.checkVariable[i].m_motPhieErr[j+g_setupJntNum] := m_SafetyCheck.electriAngleDev(m_slaveJointData.m_driveInput[i][j].m_electriAngleDev);
		m_SafetyCheck.checkVariable[i].m_cmdJntPos[j+g_setupJntNum] := m_slaveJointData.m_cmdJntPos[i][j];
		m_SafetyCheck.checkVariable[i].m_curJntVel[j+g_setupJntNum] := m_slaveJointData.m_curJntVel[i][j];
		m_SafetyCheck.checkVariable[i].m_filtJntCurrent[j+g_setupJntNum] := m_slaveJointData.m_filtJntCurrent[i][j];
		m_SafetyCheck.checkVariable[i].m_EtherCATState[j+g_setupJntNum] := m_SafetyCheck.EtherCATVerify(g_ArmNetID[i],g_JointNetPort[j+g_setupJntNum]);
		
		//m_SafetyCheck.AlarmVar[i].FixedPointErrLength := 
		m_SafetyCheck.checkVariable[i].m_spacePoseErr := m_SafetyCheck.poseErrCheck(m_slaveStatus.m_cmdArmPose[i], m_slaveStatus.m_curArmPose[i] );
		m_SafetyCheck.checkVariable[i].m_cmdSaltaValue[j+g_setupJntNum] := m_SafetyCheck.checkVariable[i].m_cmdJntPos[j+g_setupJntNum] - m_SafetyCheck.checkVariable[i].m_cmdJntPosLast[j+g_setupJntNum];
		m_SafetyCheck.checkVariable[i].m_cmdJntPosLast[j+g_setupJntNum] := m_SafetyCheck.checkVariable[i].m_cmdJntPos[j+g_setupJntNum];
		//m_SafetyCheck.AlarmVar[i].InstruPitch := m_slaveStatus.m_curArmPose[i].m_rot
	END_FOR

END_FOR]]></ST>
      </Implementation>
