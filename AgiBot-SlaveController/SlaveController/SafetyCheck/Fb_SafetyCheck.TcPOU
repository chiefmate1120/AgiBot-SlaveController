    <Method Name="errFlagReset" Id="{2f8c115b-de2b-4c35-92d2-fdbed32dcf1f}">
      <Declaration><![CDATA[METHOD errFlagReset : BOOL
VAR_INPUT
	i_ArmId:UDINT;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[

FOR i :=0 TO g_maxArmJoint BY 1 DO
