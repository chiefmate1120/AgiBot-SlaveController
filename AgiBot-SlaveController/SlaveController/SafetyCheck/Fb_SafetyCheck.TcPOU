<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4024.11">
  <POU Name="Fb_SafetyCheck" Id="{d7e5b419-2971-4dae-96a7-39fe3a1a6245}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK Fb_SafetyCheck
VAR_INPUT
END_VAR
VAR_OUTPUT
END_VAR
VAR
	i, j:UDINT;
	i_Var :ARRAY[1..g_maxNum] OF checkInputVariable;
	m_checkDelay :ARRAY[1..g_maxNum] OF checkDelay;
	m_checkDelayTime :checkDelayTime;
	m_checkPermit:ARRAY[1..g_maxNum] OF  checkPermit;
	
	m_ErrFlag:ARRAY[1..g_maxNum,0..g_maxArmJoint,1..3] OF BOOL;
	m_ErrCode:UDINT;
	m_ErrType:ARRAY [1..g_maxNum,1..3] OF BOOL;
	
	m_ErrNumType3,m_ErrNumType2,m_ErrNumType1:INT;
	o_ArrayErrCodeType:ARRAY [1..3] OF ARRAY [0..100] OF UDINT;
	m_ErrCodeUpdateAllowedType3,m_ErrCodeUpdateAllowedType2,m_ErrCodeUpdateAllowedType1:BOOL;
	m_ErrCodeUpdateDelayType3,m_ErrCodeUpdateDelayType2,m_ErrCodeUpdateDelayType1:TON;//延时接通
	
	o_ErrCode:ARRAY [1..3] OF UDINT;
		
	m_PromptMassBusy:BOOL;
	m_PromptFlag:ARRAY[1..g_maxNum,1..20] OF UINT;
	m_deleteCode:UDINT;
	
	m_commuEtherCATState:FB_EcGetSlaveState;
	m_EcSlaveState : ST_EcSlaveState;
	
	m_crcVerify : FB_EcGetSlaveCrcError;
	m_crcError : ST_EcCrcError;
	
END_VAR


VAR CONSTANT
	m_codeUpdateDelay:TIME:=T#0.1S;
	
	//Fault level: 1 indicates  prompt message, 2 indicates a recoverable fault and 3 indicates an unrecoverable fault
	m_promptMessageLevel : UINT :=1;
	m_recoverableLevel : UINT :=2;
	m_unrecoverableLevel : UINT :=3;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[
]]></ST>
    </Implementation>
    <Folder Name="interface" Id="{17cc820b-439e-4bcd-b035-470b65e01b6b}" />
    <Method Name="AlarmInit" Id="{fcbf4986-dc75-4870-b906-36e5ea23ba20}">
      <Declaration><![CDATA[METHOD AlarmInit :BOOL
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[

m_checkDelayTime.Ti_CarDualEncDev:=T#0.02S;
m_checkDelayTime.Ti_CarDriveErr:=T#1S;
m_checkDelayTime.Ti_CarActSec:=T#10S;
m_checkDelayTime.Ti_CarJntStatus:=T#1S;
m_checkDelayTime.Ti_CarTorMotor:=T#20S;
m_checkDelayTime.Ti_CaroutofLimit:=T#0.1S;
m_checkDelayTime.Ti_CarFollowDev:=T#0.02S;
m_checkDelayTime.Ti_CarJntVel:=T#0.05S;
m_checkDelayTime.Ti_CarJntCurrent:=T#1S;
m_checkDelayTime.Ti_CarEtherCAT:=T#0S;
m_checkDelayTime.Ti_CarMZDriveErr:=T#1S;



AlarmInit:=TRUE;
]]></ST>
      </Implementation>
    </Method>
    <Method Name="armsSafetyCheck" Id="{09c2f1a9-8e0a-4d09-a292-1a371f055e17}">
      <Declaration><![CDATA[METHOD armsSafetyCheck : BOOL
VAR_INPUT
	i_ArmId:UDINT;
END_VAR

VAR
	k:INT;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[
//EmergencyStopButton
IF NOT i_Var[i_ArmId].EmergencyStop OR m_ErrFlag[i_armId,0,3] THEN
	errCodeAssign(i_ArmId, 16#0C, 0, 0, 16#3001, m_unrecoverableLevel);
	RETURN;
END_IF


//驱动器状态监测、驱动器未连接
FOR i:=1 TO g_maxArmNum BY 1 DO
	m_checkDelay[i_ArmId].T_JntStatus[i](IN:= (i_Var[i_ArmId].JntStatus[i]=0 AND m_checkPermit[i_ArmId].JntStatus[i]), PT:=m_checkDelayTime.Ti_jntStatusErr , Q=> , ET=> );
	m_checkDelay[i_ArmId].T_TorMotor[i](IN:= (i_Var[i_ArmId].ActJntTorqueIO[i]=i_Var[i_ArmId].ActJntTorqueIOlast[i] AND m_checkPermit[i_ArmId].currentIsNotUpdate[i]), PT:=m_checkDelayTime.Ti_currentIsNotUpdate , Q=> , ET=> );
	IF m_checkDelay[i_ArmId].T_JntStatus[i].Q OR m_checkDelay[i_ArmId].T_TorMotor[i].Q THEN
		errCodeAssign(i_ArmId, i_ArmId+3,  i, 0, 16#3002 ,m_unrecoverableLevel );
	END_IF
END_FOR
i_Var[i_ArmId].ActJntTorqueIOlast:=i_Var[i_ArmId].ActJntTorqueIO;


//关节编码器监测
FOR i:=1 TO g_maxArmNum BY 1 DO
	m_checkDelay[i_ArmId].T_ActSec[i](IN:= (i_Var[i_ArmId].ActJntSecIO[i]=g_maxLinkEnc[i] OR i_Var[i_ArmId].ActJntSecIO[i]=g_minLinkEnc[i]) AND m_checkPermit[i_ArmId].linkEncoder[i], PT:=m_checkDelayTime.Ti_linkEncErr , Q=> , ET=> );
	IF m_checkDelay[i_ArmId].T_ActSec[i].Q THEN
		errCodeAssign(i_ArmId, i_ArmId+3, i, 0,  16#3003 ,m_unrecoverableLevel );
	END_IF
END_FOR


//驱动器错误检测
FOR i:=1 TO g_maxArmNum BY 1 DO
	m_checkDelay[i_ArmId].T_DriveErr[i](IN:= (i_Var[i_ArmId].DriveErrWord[i] AND 16#20)=g_driveErrWord AND m_checkPermit[i_ArmId].DriveErr[i] , PT:=m_checkDelayTime.Ti_driveErr , Q=> , ET=> );
	IF m_checkDelay[i_ArmId].T_DriveErr[i].Q THEN
		errCodeAssign(i_ArmId, i_ArmId+3, i, 0, 16#3004 ,m_unrecoverableLevel );
	END_IF
END_FOR


//双编码器偏差过大检测
FOR i:=1 TO g_maxArmNum BY 1 DO
	m_checkDelay[i_ArmId].T_DualEncoderDev[i](IN:= ABS(i_Var[i_ArmId].ActJntSecPos[i]-i_Var[i_ArmId].ActJntPos[i])>=g_armMaxJntEncErr[i] AND m_checkPermit[i_ArmId].DualEncoderDev[i], PT:=m_checkDelayTime.Ti_armJntEncErr , Q=> , ET=> );
	IF m_checkDelay[i_ArmId].T_DualEncoderDev[i].Q THEN
		errCodeAssign( i_ArmId, i_ArmId+3, i, 0, 16#3005, m_unrecoverableLevel );
	END_IF
END_FOR


//关节运动超限位
FOR i:=1 TO g_maxArmNum BY 1 DO
	m_checkDelay[i_ArmId].T_outofLimit[i](IN:= (i_Var[i_ArmId].ActJntPos[i]>=g_maxJntPos[i_ArmId,i] OR i_Var[i_ArmId].ActJntPos[i]<=g_minJntPos[i_ArmId,i]) AND m_checkPermit[i_ArmId].outofLimit[i] , PT:=m_checkDelayTime.Ti_outofLimit , Q=> , ET=> );
	IF m_checkDelay[i_ArmId].T_outofLimit[i].Q THEN
		errCodeAssign(i_ArmId, i_ArmId+3, i, 0, 16#3006, m_unrecoverableLevel );
	END_IF
END_FOR


//电机电角度偏差
FOR i:=1 TO g_maxArmNum BY 1 DO
	IF ABS(i_Var[i_ArmId].MotPhieErr[i])> g_motPhieErrLimit  AND m_checkPermit[i_ArmId].MotPhieErr[i] THEN
		errCodeAssign(i_ArmId, i_ArmId+3, i, 0, 16#3008, m_unrecoverableLevel );
	END_IF
END_FOR


//StopButton
IF NOT i_Var[i_ArmId].SlaveStopButton OR i_Var[i_ArmId].PCStopButton=0 OR i_Var[i_ArmId].MasterStopButton THEN
	errCodeAssign(i_ArmId, 16#0C, 0, 0, 16#2001, m_recoverableLevel );
END_IF


//关节跟随误差过大
FOR i:=1 TO g_maxArmNum BY 1 DO
	m_checkDelay[i_ArmId].T_FollowDev[i](IN:= ABS(i_Var[i_ArmId].cmdJntPos[i]- i_Var[i_ArmId].curJntPos[i])>g_armMaxJntFollowErr[i]  AND m_checkPermit[i_ArmId].followErr[i], PT:=m_checkDelayTime.Ti_armJntFollowErr , Q=> , ET=> );
	IF m_checkDelay[i_ArmId].T_FollowDev[i].Q THEN
		errCodeAssign(i_ArmId, i_ArmId+3, i, 0, 16#2002, m_recoverableLevel );
	END_IF
END_FOR


//关节速度监控
FOR i:=1 TO g_maxArmNum BY 1 DO
	m_checkDelay[i_ArmId].T_JntVel[i](IN:= ABS (i_Var[i_ArmId].ActJntVel[i])>g_safetyJntVel[i] AND m_checkPermit[i_ArmId].JntVel[i], PT:=m_checkDelayTime.Ti_jntVelErr , Q=> , ET=> );
	IF m_checkDelay[i_ArmId].T_JntVel[i].Q THEN
		errCodeAssign(i_ArmId, i_ArmId+3, i, 0, 16#2003, m_recoverableLevel );
	END_IF
END_FOR


//笛卡尔空间指令与位置偏差检测
FOR i:=1 TO 6 BY 1 DO
	m_checkDelay[i_ArmId].T_SpacePosErr[i](IN:= i_Var[i_ArmId].SpacePoseErr[i]> g_maxCartPoseErr[i] AND m_checkPermit[i_ArmId].SpacePosErr[i], PT:=m_checkDelayTime.Ti_spacePoseErr , Q=> , ET=> );
	IF m_checkDelay[i_ArmId].T_SpacePosErr[i].Q THEN
		errCodeAssign(i_ArmId, i_ArmId+3, i, 1, 16#2004, m_recoverableLevel );
	END_IF
END_FOR


//电流超限监测
FOR i:=1 TO g_maxArmNum BY 1 DO
	m_checkDelay[i_ArmId].T_JntCurrent[i](IN:= ABS(i_Var[i_ArmId].filtJntCurrent[i])>g_maxJntCurrent[i] AND m_checkPermit[i_ArmId].JntCurrent[i], PT:=m_checkDelayTime.Ti_jntCurrentErr , Q=> , ET=> );
	IF m_checkDelay[i_ArmId].T_JntCurrent[i].Q THEN
		errCodeAssign(i_ArmId, i_ArmId+3, i, 0, 16#2005, m_recoverableLevel );
	END_IF
END_FOR


//Ethercat通讯检测
FOR i:=1 TO g_maxArmNum BY 1 DO
	IF NOT i_Var[i_ArmId].EtherCATState[i].EtherCATState THEN
		errCodeAssign(i_ArmId, i_ArmId+3, i, 0, 16#2006, m_recoverableLevel );
	END_IF
END_FOR


//不动点发生变化监控
m_checkDelay[i_ArmId].T_FixedPointErr(IN:=  i_Var[i_ArmId].FixedPointErrLength>g_maxFixedPointErr AND m_checkPermit[i_ArmId].FixedPointErr, PT:=m_checkDelayTime.Ti_FixedPointErr , Q=> , ET=> );
IF m_checkDelay[i_ArmId].T_FixedPointErr.Q THEN
	errCodeAssign(i_ArmId, i_ArmId+3, i, 0, 16#2007, m_recoverableLevel );
END_IF


//目标位置指令突变检测
FOR i:=1 TO g_maxArmNum BY 1 DO
	IF i_Var[i_ArmId].CmdSaltaValue[i]>g_maxJntCmdSalta[i] AND m_checkPermit[i_ArmId].CmdJntSaltation[i] THEN
		errCodeAssign(i_ArmId, i_ArmId+3, i, 0, 16#2008, m_recoverableLevel );
	END_IF
END_FOR


//器械在奇异区域内
IF (i_Var[i_ArmId].InstruPitch> g_maxInstruPitch OR i_Var[i_ArmId].InstruPitch< g_minInstruPitch) AND m_checkPermit[i_ArmId].InstruSingularity THEN
	errCodeAssign(i_ArmId, i_ArmId+3, 0, 0, 16#200E, m_recoverableLevel );
END_IF


//器械张开角度过大或过小
IF (i_Var[i_ArmId].InstruOpenAngle> g_maxInstruOpen OR i_Var[i_ArmId].InstruOpenAngle< g_minInstruOpen) AND m_checkPermit[i_ArmId].InstruOpenAngle THEN
	errCodeAssign(i_ArmId, i_ArmId+3, 0, 0, 16#200F, m_recoverableLevel );
END_IF




//提示信息
FOR i:=1 TO 20 BY 1 DO
	PromptMassCode(
		i_PromptStatus:= i_Var[i_ArmId].PromptStatus[k], 
		i_ArmId:= i_ArmId, 
		i_ErrID:= i, 
		i_ErrLocation:=i_Var[i_ArmId].ErrLocation[k] );
END_FOR




]]></ST>
      </Implementation>
    </Method>
    <Method Name="ArrayDeleteFrist" Id="{00e49c9f-cbe8-4b26-b05a-c4b6acb31969}">
      <Declaration><![CDATA[METHOD ArrayDeleteFrist : ARRAY [0..100] OF UDINT;
VAR_INPUT
	i_Array:ARRAY [0..100] OF UDINT;
END_VAR

VAR
	k:INT;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[
ArrayDeleteFrist[100]:=0;

FOR k:=0 TO 99 BY 1 DO
	ArrayDeleteFrist[k]:=i_Array[k+1];
END_FOR]]></ST>
      </Implementation>
    </Method>
    <Method Name="ArrayDeleteInput" Id="{980f1673-9d6d-4af2-a068-78f386b10642}">
      <Declaration><![CDATA[METHOD ArrayDeleteInput : BOOL
VAR_INPUT
	i_Array:ARRAY[0..100] OF UDINT;
	i_DeleteCode:UDINT;
END_VAR

VAR_OUTPUT
	o_Array:ARRAY[0..100] OF UDINT;
	
END_VAR

VAR
	i:INT;
	Falg:BOOL;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[
FOR i:=0 TO 20 BY 1 DO 
	IF i_Array[i]=i_DeleteCode THEN
		Falg:=TRUE;
	END_IF
	
	IF Falg=TRUE THEN
		o_Array[i]:=i_Array[i+1];
	ELSE
		o_Array[i]:=i_Array[i];
	END_IF
END_FOR
o_Array[20]:=0;]]></ST>
      </Implementation>
    </Method>
    <Method Name="ArrayFIFO" Id="{655e2224-db9d-4b4a-8461-cda6c8a0a0dc}">
      <Declaration><![CDATA[METHOD ArrayFIFO : ARRAY [0..100] OF  UDINT
VAR_INPUT
	i_Array:ARRAY[0..100] OF UDINT;
	i_NewData:UDINT;
END_VAR

VAR
	k:INT;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[
FOR k:=0 TO 20 BY 1 DO
	ArrayFIFO[k+1]:=i_Array[k];
END_FOR

ArrayFIFO[0]:=i_NewData;]]></ST>
      </Implementation>
    </Method>
    <Method Name="ArrayFILO" Id="{26262d95-ef1c-497d-b548-c28fcd1771c9}">
      <Declaration><![CDATA[METHOD ArrayFILO : ARRAY [0..100] OF  UDINT
VAR_INPUT
	i_Array:ARRAY[0..100] OF UDINT;
	i_NewData:UDINT;
END_VAR

VAR
	k:INT;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[
FOR k:=0 TO 20 BY 1 DO
	ArrayFILO[k]:=i_Array[k];
	IF i_Array[k]=0 THEN
		ArrayFILO[k]:=i_NewData;
		EXIT;
	END_IF
END_FOR

]]></ST>
      </Implementation>
    </Method>
    <Method Name="cartSafetyCheck" Id="{64489d60-54d3-406d-a82e-df5d6a98f0f1}">
      <Declaration><![CDATA[METHOD cartSafetyCheck : BOOL
VAR_INPUT
	i_ArmId:UDINT;
END_VAR


VAR
	k:INT;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[
(*
//驱动器状态监测、驱动器未连接
FOR i:=1 TO g_maxCartJoint BY 1 DO
	m_checkDelay[i_ArmId].T_CarJntStatus[i](IN:= (i_Var[i_ArmId].CarJntStatus[i]=0 AND m_checkPermit[i_ArmId].CarJntStatus[i]), PT:=m_checkDelayTime.Ti_CarJntStatus , Q=> , ET=> );
	//m_checkDelay[i_ArmId].T_CarTorMotor[i](IN:= (i_Var[i_ArmId].CarActJntTorqueIO[i]=i_Var[i_ArmId].CarActJntTorqueIOlast[i] AND m_checkPermit[i_ArmId].CarTorMotor[i]), PT:=m_checkDelayTime.Ti_CarTorMotor , Q=> , ET=> );
	IF m_checkDelay[i_ArmId].T_CarJntStatus[i].Q OR m_checkDelay[i_ArmId].T_CarTorMotor[i].Q THEN
		m_ErrLocation:=i_ArmId+3;  m_ErrJoint:=i;  m_ErrID:=16#3002; m_ErrFlagID:=3;
		ErrCodeAssign(i_ArmId:=i_ArmId,i_ErrLocation:= m_ErrLocation, i_ErrJoint:= m_ErrJoint,i_ErrCodeMode:=0, i_ErrID:= m_ErrID );
	END_IF
END_FOR
i_Var[i_ArmId].CarActJntTorqueIOlast:=i_Var[i_ArmId].CarActJntTorqueIO;


//关节编码器监测
FOR i:=1 TO g_maxCartJoint BY 1 DO
	m_checkDelay[i_ArmId].T_CarActSec[i](IN:= (i_Var[i_ArmId].CarActJntSecIO[i]>=m_MonitorThreshold.CarMaxSecEnc[i] OR i_Var[i_ArmId].CarActJntSecIO[i]<=m_MonitorThreshold.CarMinSecEnc[i]) AND m_checkPermit[i_ArmId].CarActSec[i], PT:=m_checkDelayTime.Ti_CarActSec , Q=> , ET=> );
	IF m_checkDelay[i_ArmId].T_CarActSec[i].Q THEN
		m_ErrLocation:=i_ArmId+3;  m_ErrJoint:=i;  m_ErrID:=16#3003; m_ErrFlagID:=3;
		ErrCodeAssign(i_ArmId:=i_ArmId,i_ErrLocation:= m_ErrLocation, i_ErrJoint:= m_ErrJoint,i_ErrCodeMode:=0, i_ErrID:= m_ErrID );
	END_IF
END_FOR


//驱动器错误检测
FOR i:=1 TO g_maxCartJoint BY 1 DO
	m_checkDelay[i_ArmId].T_CarDriveErr[i](IN:= (i_Var[i_ArmId].CarDriveErrWord[i] AND 16#20)=32 AND m_checkPermit[i_ArmId].CarDriveErr[i] , PT:=m_checkDelayTime.Ti_CarDriveErr , Q=> , ET=> );
	IF m_checkDelay[i_ArmId].T_CarDriveErr[i].Q THEN
		m_ErrLocation:=i_ArmId+3;  m_ErrJoint:=i;  m_ErrID:=16#3004; m_ErrFlagID:=3;
		ErrCodeAssign(i_ArmId:=i_ArmId,i_ErrLocation:= m_ErrLocation, i_ErrJoint:= m_ErrJoint,i_ErrCodeMode:=0, i_ErrID:= m_ErrID );
	END_IF
END_FOR



//台车双编码器偏差过大
FOR i:=1 TO g_maxCartJoint BY 1 DO
	m_checkDelay[i_ArmId].T_CarDualEncoderDev[i](IN:= ABS (i_Var[i_ArmId].CarActJntSecPos[i]-i_Var[i_ArmId].CarActJntPos[i])>m_MonitorThreshold.CarDevSecAndMotorLimit[i] AND m_checkPermit[i_ArmId].CarDualEncoderDev[i], PT:=m_checkDelayTime.Ti_CarDualEncDev , Q=> , ET=> );
	IF m_checkDelay[i_ArmId].T_CarDualEncoderDev[i].Q THEN
		m_ErrLocation:=i_ArmId+3;  m_ErrJoint:=i;  m_ErrID:=16#3005; m_ErrFlagID:=3;
		ErrCodeAssign(i_ArmId:=i_ArmId,i_ErrLocation:= m_ErrLocation, i_ErrJoint:= m_ErrJoint,i_ErrCodeMode:=0, i_ErrID:= m_ErrID );
	END_IF
END_FOR


//关节运动超限位
FOR i:=1 TO g_maxCartJoint BY 1 DO
	m_checkDelay[i_ArmId].T_CaroutofLimit[i](IN:= (i_Var[i_ArmId].CarActJntPosDH[i]>=m_MonitorThreshold.CarMaxJntLimit[i] OR i_Var[i_ArmId].CarActJntPosDH[i]<=m_MonitorThreshold.CarMinJntLimit[i]) AND m_checkPermit[i_ArmId].CaroutofLimit[i] , PT:=m_checkDelayTime.Ti_CaroutofLimit , Q=> , ET=> );
	IF m_checkDelay[i_ArmId].T_CaroutofLimit[i].Q THEN
		m_ErrLocation:=i_ArmId+3;  m_ErrJoint:=i;  m_ErrID:=16#3006; m_ErrFlagID:=3;
		ErrCodeAssign(i_ArmId:=i_ArmId,i_ErrLocation:= m_ErrLocation, i_ErrJoint:= m_ErrJoint,i_ErrCodeMode:=0, i_ErrID:= m_ErrID );
	END_IF
END_FOR


//StopButton
IF NOT i_Var[i_ArmId].SlaveStopButton OR i_Var[i_ArmId].PCStopButton=0 OR i_Var[i_ArmId].MasterStopButton THEN
	m_ErrLocation:=16#FF; m_ErrJoint:=0; m_ErrID:=16#2001; m_ErrFlagID:=2;
	ErrCodeAssign(i_ArmId:=i_ArmId,i_ErrLocation:= m_ErrLocation, i_ErrJoint:= m_ErrJoint,i_ErrCodeMode:=0, i_ErrID:= m_ErrID );
END_IF

//关节跟随误差过大
FOR i:=1 TO g_maxCartJoint BY 1 DO
	m_checkDelay[i_ArmId].T_CarFollowDev[i](IN:= ABS(i_Var[i_ArmId].CarcmdJntPos[i]- i_Var[i_ArmId].CarcurJntPos[i])>m_MonitorThreshold.CarFollowDeviationLimit[i]  AND m_checkPermit[i_ArmId].CarFollowDev[i], PT:=m_checkDelayTime.Ti_CarFollowDev , Q=> , ET=> );
	IF m_checkDelay[i_ArmId].T_CarFollowDev[i].Q THEN
		m_ErrLocation:=i_ArmId+3;  m_ErrJoint:=i;  m_ErrID:=16#2002; m_ErrFlagID:=2;
		ErrCodeAssign(i_ArmId:=i_ArmId,i_ErrLocation:= m_ErrLocation, i_ErrJoint:= m_ErrJoint,i_ErrCodeMode:=0, i_ErrID:= m_ErrID );
	END_IF
END_FOR


//关节速度监控
FOR i:=1 TO MaxCarJoint BY 1 DO
	m_checkDelay[i_ArmId].T_CarJntVel[i](IN:= ABS(i_Var[i_ArmId].CarActJntVel[i])>m_MonitorThreshold.CarJntVelLimit[i] AND m_checkPermit[i_ArmId].CarJntVel[i], PT:=m_checkDelayTime.Ti_CarJntVel , Q=> , ET=> );
	IF m_checkDelay[i_ArmId].T_CarJntVel[i].Q THEN
		m_ErrLocation:=i_ArmId+3;  m_ErrJoint:=i;  m_ErrID:=16#2003; m_ErrFlagID:=2;
		ErrCodeAssign(i_ArmId:=i_ArmId,i_ErrLocation:= m_ErrLocation, i_ErrJoint:= m_ErrJoint,i_ErrCodeMode:=0, i_ErrID:= m_ErrID );
	END_IF
END_FOR


//电流超限监测
FOR i:=1 TO g_maxCartJoint BY 1 DO
	m_checkDelay[i_ArmId].T_CarJntCurrent[i](IN:= ABS(i_Var[i_ArmId].CarMorCurrentFilter[i])>m_MonitorThreshold.CarJntElectricityLimit[i] AND m_checkPermit[i_ArmId].CarJntCurrent[i], PT:=m_checkDelayTime.Ti_CarJntCurrent , Q=> , ET=> );
	IF m_checkDelay[i_ArmId].T_CarJntCurrent[i].Q THEN
		m_ErrLocation:=i_ArmId+3;  m_ErrJoint:=i;  m_ErrID:=16#2005; m_ErrFlagID:=2;
		ErrCodeAssign(i_ArmId:=i_ArmId,i_ErrLocation:= m_ErrLocation, i_ErrJoint:= m_ErrJoint,i_ErrCodeMode:=0, i_ErrID:= m_ErrID );
	END_IF
END_FOR


//Ethercat通讯检测
FOR i:=1 TO g_maxCartJoint BY 1 DO
	m_checkDelay[i_ArmId].T_CarEtherCAT[i](IN:= NOT i_Var[i_ArmId].CarEtherCATState[i], PT:=m_checkDelayTime.Ti_CarEtherCAT , Q=> , ET=> );
	IF m_checkDelay[i_ArmId].T_CarEtherCAT[i].Q THEN
		m_ErrLocation:=i_ArmId+3;  m_ErrJoint:=i;  m_ErrID:=16#2006; m_ErrFlagID:=2;
		ErrCodeAssign(i_ArmId:=i_ArmId,i_ErrLocation:= m_ErrLocation, i_ErrJoint:= m_ErrJoint,i_ErrCodeMode:=0, i_ErrID:= m_ErrID );
	END_IF
END_FOR


//鸣志驱动器错误检测
FOR i:=1 TO g_maxCartJoint BY 1 DO
	m_checkDelay[i_ArmId].T_CarMingzhiDriveErr[i](IN:= i_Var[i_ArmId].CarJntStatus[i]=5  AND m_checkPermit[i_ArmId].CarMZDriveErr[i] , PT:=m_checkDelayTime.Ti_CarMZDriveErr , Q=> , ET=> );
	IF m_checkDelay[i_ArmId].T_CarMingzhiDriveErr[i].Q THEN
		m_ErrLocation:=i_ArmId+3;  m_ErrJoint:=i;  m_ErrID:=16#2009; m_ErrFlagID:=2;
